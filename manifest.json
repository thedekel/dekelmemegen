{
  "name": "Dekel Meme Generator",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "description",

  "background": {
    "page": "background.html"
  },
  "content_scripts": [
    {
    "matches": ["*://*/*"],
    "js": ["chrombsContent.js"]
    }
  ],
  "omnibox": { "keyword": "chromb" },
  "options_page": "options.html",
  "permissions": [
    "*://*/*",
    "http://*/",
    "contextMenus"
  ]
}
